{
	"name": "vinabb/stylesdemo",
	"type": "phpbb-extension",
	"description": "Style preview utility for demo phpBB sites.",
	"homepage": "http://vinabb.vn/",
	"version": "1.1.3",
	"time": "2016-09-06",
	"license": "GPL-2.0",
	"authors": [{
		"name": "Khoa Nguyen",
		"email": "nedka@vinabb.vn",
		"homepage": "http://nedka.vn/",
		"role": "Developer"
	}],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Styles Demo",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.2,<3.2.0@dev"
		},
		"version-check": {
			"host": "raw.githubusercontent.com",
			"directory": "/VinaBB/Styles.Demo/1.1.x",
			"filename": "update.json",
			"ssl": true
		}
	}
